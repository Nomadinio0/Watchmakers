<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Watchmakers</title>
    <link rel="stylesheet" href="/dist/css/style.min.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,400;0,700;1,200&display=swap"
        rel="stylesheet">
</head>

<body>
    <!-- @include 'nav.kit' -->
    <section class="offers wrapper">
        <div class="pricelist">
            <h2 class="section__heading">Cennik usług</h2>
            <table class="pricelist__table">
                <thead>
                    <tr>
                        <th>Nazwa usługi</th>
                        <th>Cena</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Wymiana baterii</td>
                        <td>20zł</td>
                    </tr>
                    <tr>
                        <td>Wymiana szkła</td>
                        <td>50zł</td>
                    </tr>
                    <tr>
                        <td>Wymiana paska</td>
                        <td>10zł</td>
                    </tr>
                    <tr>
                        <td>Renowacja zegarka kwarcowego</td>
                        <td>200zł</td>
                    </tr>
                    <tr>
                        <td>Renowacja zegarka mechanicznego</td>
                        <td>250zł</td>
                    </tr>
                    <tr>
                        <td>Renowacja zegara wahadłowego</td>
                        <td>400zł</td>
                    </tr>
                </tbody>
            </table>
        </div>
        <div class="auctions wrapper">
            <h2 class="section__heading">Aukcje</h2>
            <div class="auctions__cards">
                <div class="auctions__card">
                    <div class="auctions__card-img"></div>
                </div>
                <div class="auctions__card"></div>
                <div class="auctions__card"></div>
            </div>
        </div>
    </section>
    <!-- @include 'footer.kit' -->
    <script src="/dist/js/main.min.js"></script>
</body>

</html>